<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane fx:id="mainAnchor" prefHeight="510.0" prefWidth="860.0" styleClass="logo-panel" stylesheets="@CSS/signup.css" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="bank.fx.bank.Controller.mainController">
   <children>
      <ImageView fitHeight="150.0" fitWidth="200.0" layoutX="330.0" layoutY="98.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@images/logo.png" />
         </image>
      </ImageView>
      <Button layoutX="374.0" layoutY="274.0" mnemonicParsing="false" onAction="#switchToRegister" prefHeight="30.0" prefWidth="112.0" styleClass="deposit, mainBtn" stylesheets="@CSS/dashboard.css" text="Register" />
      <Button layoutX="374.0" layoutY="216.0" mnemonicParsing="false" onAction="#switchToLogin" prefHeight="30.0" prefWidth="112.0" styleClass="deposit, mainBtn" stylesheets="@CSS/dashboard.css" text="Login" />
      <Button layoutX="374.0" layoutY="333.0" mnemonicParsing="false" onAction="#exit" prefHeight="30.0" prefWidth="112.0" styleClass="deposit, mainBtn" stylesheets="@CSS/dashboard.css" text="Exit" />
   </children>
</AnchorPane>
